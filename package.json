{
  "name": "indie-festivals-api",
  "version": "1.0.0",
  "description": "API to showcase indie game festivals data synced from Google Sheets",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "sync": "ts-node src/scripts/syncData.ts",
    "sync:sheets": "ts-node src/scripts/syncGoogleSheets.ts",
    "sync:sheets:csv": "ts-node src/scripts/syncGoogleSheets.ts --csv-only",
    "enrich": "ts-node src/scripts/enrich.ts",
    "enrich:ai": "ts-node src/scripts/aiEnrichFestivals.ts",
    "enrich:ai:force": "ts-node src/scripts/aiEnrichFestivals.ts --force",
    "enrich:ai:retry": "ts-node src/scripts/aiEnrichFestivals.ts --retry-failed",
    "enrich:slugs": "ts-node src/scripts/aiEnrichFestivals.ts --slugs-only",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "vercel-build": "tsc"
  },
  "engines": {
    "node": ">=18.x"
  },
  "keywords": [
    "indie",
    "games",
    "festivals",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/swagger-ui-express": "^4.1.8",
    "@vercel/node": "^3.2.29",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "mongoose": "^8.7.0",
    "node-fetch": "^2.7.0",
    "public-google-sheets-parser": "^1.5.4",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "@types/node-fetch": "^2.6.13",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
